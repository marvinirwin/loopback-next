{
  "name": "@loopback/docs",
  "version": "1.21.1",
  "description": "Documentation for LoopBack 4",
  "homepage": "https://github.com/strongloop/loopback-next/tree/master/docs",
  "author": "IBM Corp.",
  "engines": {
    "node": ">=8.9"
  },
  "files": [
    "**/*"
  ],
  "keywords": [
    "LoopBack",
    "docs"
  ],
